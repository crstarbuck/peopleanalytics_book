### Data Prep ###
# Set seed for reproducibility
set.seed(12345)
# Turnover rates
reg_rt <- round(runif(24, 5, 15), 1)
nonreg_rt <- round(runif(24, 1, 5), 1)
vol_rt <- reg_rt + nonreg_rt
turnover_dat <- data.frame(month = 1:24,
reg_rt = reg_rt,
nonreg_rt = nonreg_rt,
vol_rt = vol_rt)
ggplot2::ggplot(turnover_dat) +
